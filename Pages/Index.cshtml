                <aside class="col-md-3">
                    <div class="sidebar_widget">
                        <div class="widget_heading">
                            <h5><i class="fa fa-calculator" aria-hidden="true"></i> Vehicle Details </h5>
                        </div>
                        <div class="financing_calculatoe">
                            <div class="form-group">
                                <label class="form-label">Market Value Determination ($)</label><br />$
                                @if (Model.report.signature.fairmarketvalue != null)
                                {
                                    @Model.report.signature.fairmarketvalue
                                }
                            </div>
                            <div class="form-group">
                                <label class="form-label">Year</label><br />
                                @if (Model.report.basicInfo.year != null)
                                {
                                    @Model.report.basicInfo.year
                                }
                            </div>
                            <div class="form-group">
                                <label class="form-label">Make</label><br />
                                @if (Model.report.basicInfo.make != null)
                                {
                                    @Model.report.basicInfo.make
                                }
                            </div>
                            <div class="form-group">
                                <label class="form-label">Model</label><br />
                                @if (Model.report.basicInfo.model != null)
                                {
                                    @Model.report.basicInfo.model
                                }
                            </div>
                            <div class="form-group">
                                <label class="form-label">Color</label>
                                <br />
                                @if (Model.report.basicInfo.color != null)
                                {
                                    @Model.report.basicInfo.color
                                }
                            </div>
                            <div class="form-group">
                                <label class="form-label">VIN</label>
                                <br />
                                @Model.report.basicInfo.vin.ToUpper()
                            </div>
                        </div>
                    </div>


                    <div class="sidebar_widget">
                        <div class="widget_heading">
                            <h5><i class="fa fa-address-card-o" aria-hidden="true"></i> Appraiser Info </h5>
                        </div>
                        <div class="dealer_detail">
                            <p><span>Name:</span> Corey Lamar</p>
                            <p><span>Email:</span> corey@aaperfected.com</p>
                            <p><span>Phone:</span> 404-454-1265</p>
                        </div>
                    </div>
                </aside>
                <!--/Side-Bar-->
            </div>

            <div class="space-20"></div>
            <div class="divider"></div>

            <!--Similar-Cars-->
            <div class="similar_cars">
                <h3>Hagerty</h3>
                <div class="row">
                    <div class="col-md-12 grid_listing">
                        <div role="tabpanel" class="tab-pane" id="specification">
                            <div class="table-responsive">
                                <!--Basic-Info-Table-->
                                <table>
                                    <thead>
                                        <tr>
                                            <th colspan="2">Hagerty</th>
                                        </tr>
                                    </thead>
                                    <tbody>
