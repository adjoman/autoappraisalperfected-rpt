                <h3>KBB</h3>
                <div class="row">
                    &nbsp;
                </div>
            </div>
            <!--/Similar-Cars-->
            <!--Similar-Cars-->
            <div class="similar_cars">
                <h3>Certificate</h3>
                <div class="row">
                    <div class="col-md-12 grid_listing">
                        <div class="product-listing-m gray-bg">
                            <div class="product-listing-img">
                                <a href="#"><img src="img/appraisal-certificate.png" class="img-responsive" alt="image" /> </a>
                                &nbsp;
                            </div>
                            <div class="product-listing-content">
                                <h5><a href="#">Certified Appraiser</a></h5>
                                <p class="list-price">License #: 107748</p>
                                <div class="car-location"><span><i class="fa fa-map-marker" aria-hidden="true"></i> Atlanta, USA</span></div>
                                <ul class="features_list">
                                    <li>&nbsp;</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/Similar-Cars-->

        </div>
    </section>
    <!--/Listing-detail-->
    <!--Brands-->
    <section class="brand-section gray-bg">
        <div class="container">
            <div class="brand-hadding">
                <h5>Photos</h5>
            </div>
            <div class="brand-logo-list">
