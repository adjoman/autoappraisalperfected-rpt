        <div class="container">
            &nbsp;
        </div>
    </section>

    <!--Listing-detail-->
    <section class="listing-detail">
        <div class="container">
            <div class="listing_detail_head row">
                <div class="col-md-9">
                    @if (Model.report.basicInfo.model != null)
                    {
                        <h2>@Model.report.basicInfo.make @Model.report.basicInfo.model </h2>
                    }
                    <h3>@Model.report.client.customer_name</h3>
                    @if (Model.report.basicInfo.vin != null)
                    {
                        <h2>VIN: @Model.report.basicInfo.vin.ToUpper() </h2>
                    }
                    <div class="car-location">
                        <span>
                            <i class="fa fa-map-marker" aria-hidden="true"></i>
                            @if (Model.report.basicInfo.location != null)
                            {
                                @Model.report.basicInfo.location
                            }
                        </span>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="price_info">
                        @if (Model.report.signature.fairmarketvalue != null)
                        {
                            <p>$@Model.report.signature.fairmarketvalue</p>
                        }
                    </div>
                    <p>Market Value Determination</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-9">
                    <div class="main_features">
                        <ul>
                            <li>
                                <i class="fa fa-tachometer" aria-hidden="true"></i>
                                @if (Model.report.basicInfo.mileage != null)
                                {
                                    <h5>@Model.report.basicInfo.mileage</h5>
                                }
                                <p>Total Miles</p>
                            </li>
                            <li>
                                <i class="fa fa-calendar" aria-hidden="true"></i>
                                @if (Model.report.basicInfo.year != null)
                                {
                                    <h5>@Model.report.basicInfo.year</h5>
                                }
                                <p>Year</p>
                            </li>
                            <li>
                                <i class="fa fa-power-off" aria-hidden="true"></i>
                                @if (Model.report.transmission.transmission_type != null)
                                {
                                    <h5>@Model.report.transmission.transmission_type</h5>
                                }
                                <p>Transmission</p>
                            </li>

                        </ul>
                    </div>
                    <div class="listing_more_info">
                        <div class="listing_detail_wrap">
                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs gray-bg" role="tablist">
                                <li role="presentation"><a href="#vehicle-overview " aria-controls="vehicle-overview" role="tab" data-toggle="tab">Vehicle Data </a></li>
                            </ul>

                            <!-- Tab panes -->
                            <div class="similar_cars">
                                <div class="row">
                                    <div class="col-md-12 grid_listing">
                                        <div role="tabpanel" class="tab-pane" id="specification">
                                            <div class="table-responsive">
                                                <!--Basic-Info-Table-->
                                                @if (Model.VinDecoded.Count >= 1)
                                                {
                                                    <table>
                                                        <tbody>
                                                            @foreach (var item in Model.VinDecoded.Results)
                                                            {
