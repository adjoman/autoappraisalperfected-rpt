                                            <thead>
                                                <tr>
                                                    <th colspan="2">BASIC INFO</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>Model Year</td>
                                                    @if (Model.report.basicInfo.year != null)
                                                    {
                                                        <td>@Model.report.basicInfo.year</td>
                                                    }
                                                </tr>
                                                <tr>
                                                    <td>No. of Owners</td>
                                                    <td>4</td>
                                                </tr>
                                                <tr>
                                                    <td>KMs Driven</td>
                                                    <td>30,000</td>
                                                </tr>
                                                <tr>
                                                    <td>Fuel Type</td>
                                                    <td>Gas</td>
                                                </tr>
                                            </tbody>
                                        </table>

                                        <!--Technical-Specification-Table-->
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th colspan="2">Technical Specification</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>Engine Type</td>
                                                    <td>TDCI Diesel Engine</td>
                                                </tr>
                                                <tr>
                                                    <td>Engine Description</td>
                                                    <td>1.5KW</td>
                                                </tr>
